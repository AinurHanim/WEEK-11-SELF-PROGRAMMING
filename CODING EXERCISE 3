package MyMethod;
import java.util.Scanner;
public class NoteSlide25 {
	
	public static void printSquare(int x)
	{
		System.out.println(x*x);
	}
	
	public static void main(String[] arguments)
	{
		int value=2;
		printSquare(value); //2*2
		printSquare(3); //3*3
		printSquare(value*2); //(2*2)*4
	}

}
